		private void comboCATPort_SelectedIndexChanged(object sender, System.EventArgs e)
		{
            try
            {
                if (comboCATPort.Text.StartsWith("COM"))
                {
                    string delim = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
                    console.CATPort = Int32.Parse(comboCATPort.Text.Trim(delim.ToCharArray()));
                }
            }
            catch (Exception ex)
            {
                MessageBox.Show("Error in CAT Port selection!\n" + ex.ToString());
            }
		}

		private void comboCATPTTPort_SelectedIndexChanged(object sender, System.EventArgs e)
		{
            try
            {
                if (comboCATPTTPort.Text.StartsWith("COM"))
                {
                    string delim = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
                    console.CATPTTBitBangPort = Int32.Parse(comboCATPTTPort.Text.Trim(delim.ToCharArray()));
