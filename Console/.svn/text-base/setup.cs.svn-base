		private void udDisplayFPS_ValueChanged(object sender, System.EventArgs e)
		{
			console.DisplayFPS = (int)udDisplayFPS.Value;
		}

		private void udDisplayGridMax_ValueChanged(object sender, System.EventArgs e)
		{
			if(udDisplayGridMax.Value <= udDisplayGridMin.Value + 10)
				udDisplayGridMax.Value = udDisplayGridMin.Value + 10;

            SpectrumHighLevel = (float)udDisplayGridMax.Value;
		}

		private void udDisplayGridMin_ValueChanged(object sender, System.EventArgs e)
		{
			if(udDisplayGridMin.Value >= udDisplayGridMax.Value - 10)
				udDisplayGridMin.Value = udDisplayGridMax.Value - 10;

            SpectrumLowLevel = (float)udDisplayGridMin.Value;
		}

		private void udDisplayGridStep_ValueChanged(object sender, System.EventArgs e)
		{
			Display_GDI.SpectrumGridStep = (int)udDisplayGridStep.Value;
#if(DirectX)
            Display_DirectX.SpectrumGridStep = (int)udDisplayGridStep.Value;
#endif
		}

		private void comboDisplayLabelAlign_SelectedIndexChanged(object sender, System.EventArgs e)
		{
			switch(comboDisplayLabelAlign.Text)
			{
				case "Left":
					Display_GDI.DisplayLabelAlign = DisplayLabelAlignment.LEFT;
#if(DirectX)
                    Display_DirectX.DisplayLabelAlign = DisplayLabelAlignment.LEFT;
#endif
					break;
				case "Cntr":
					Display_GDI.DisplayLabelAlign = DisplayLabelAlignment.CENTER;
#if(DirectX)
                    Display_DirectX.DisplayLabelAlign = DisplayLabelAlignment.CENTER;
#endif
					break;
				case "Right":
					Display_GDI.DisplayLabelAlign = DisplayLabelAlignment.RIGHT;
#if(DirectX)
                    Display_DirectX.DisplayLabelAlign = DisplayLabelAlignment.RIGHT;
#endif
					break;
				case "Auto":
					Display_GDI.DisplayLabelAlign = DisplayLabelAlignment.AUTO;
#if(DirectX)
                    Display_DirectX.DisplayLabelAlign = DisplayLabelAlignment.AUTO;
#endif
					break;
				case "Off":
					Display_GDI.DisplayLabelAlign = DisplayLabelAlignment.OFF;
#if(DirectX)
                    Display_DirectX.DisplayLabelAlign = DisplayLabelAlignment.OFF;
#endif
					break;
				default:
					Display_GDI.DisplayLabelAlign = DisplayLabelAlignment.LEFT;
#if(DirectX)
                    Display_DirectX.DisplayLabelAlign = DisplayLabelAlignment.LEFT;
#endif
					break;
			}
		}

		private void udDisplayAVGTime_ValueChanged(object sender, System.EventArgs e)
		{
			double buffer_time = double.Parse(comboAudioBuffer1.Text) / (double)console.SampleRate1;
			int buffersToAvg = (int)((float)udDisplayAVGTime.Value * 0.001 / buffer_time);
			buffersToAvg = Math.Max(2, buffersToAvg);
			Display_GDI.DisplayAvgBlocks = buffersToAvg;
#if(DirectX)
            Display_DirectX.DisplayAvgBlocks = buffersToAvg;
#endif
		}

